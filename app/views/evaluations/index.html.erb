<h1>Listing evaluations</h1>

<table>
  <tr>
    <th>Answer1</th>
    <th>Answer2</th>
    <th>Answer3</th>
    <th>Answer4</th>
    <th>Answer5</th>
    <th>Answer6</th>
    <th>Answer7</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @evaluations.each do |evaluation| %>
  <tr>
    <td><%= evaluation.answer1 %></td>
    <td><%= evaluation.answer2 %></td>
    <td><%= evaluation.answer3 %></td>
    <td><%= evaluation.answer4 %></td>
    <td><%= evaluation.answer5 %></td>
    <td><%= evaluation.answer6 %></td>
    <td><%= evaluation.answer7 %></td>
    <td><%= link_to 'Show', evaluation %></td>
    <td><%= link_to 'Edit', edit_evaluation_path(evaluation) %></td>
    <td><%= link_to 'Destroy', evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Evaluation', new_evaluation_path %>
